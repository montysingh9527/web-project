{
  "name": "egg-server-mysql",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "egg": {
    "declarations": true
  },
  "scripts": {
    "migrate": "npx sequelize db:migrate && echo \"初始化数据库迁移\"",
    "start": "egg-scripts start --daemon --title=egg-server-egg-server && echo \"生产运行项目\"",
    "stop": "egg-scripts stop --title=egg-server-egg-server && echo \"停止项目\"",
    "dev": "egg-bin dev && echo \"dev启动项目\"",
    "test": "npm run lint -- --fix && npm run test:local",
    "test:local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov"
  },
  "dependencies": {
    "egg": "^3.17.5",
    "egg-cors": "^3.0.0",
    "egg-jwt": "^3.1.7",
    "egg-scripts": "2",
    "egg-sequelize": "^6.0.0",
    "md5": "^2.3.0",
    "mysql2": "^3.6.5"
  },
  "devDependencies": {
    "egg-bin": "6",
    "egg-mock": "5",
    "eslint": "8",
    "eslint-config-egg": "13",
    "sequelize-cli": "^6.6.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "",
  "license": "MIT"
}
